worker_processes  2;

events {
    use           epoll;
    worker_connections  128;
}

http {
  upstream myproject {
    server 127.0.0.1:8000 weight=3;
    server 127.0.0.1:8001;
    server 127.0.0.1:8002;
    server 127.0.0.1:8003;
  }

  server {
    listen 80;

    location / {
      proxy_pass http://server:3000;
    }
  }
}
